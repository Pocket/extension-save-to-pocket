  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/.tooling/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/.tooling/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/.tooling/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "parser": "babylon",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "scripts": {
    "start": "node .tooling/scripts/start.js",
    "build": "node .tooling/scripts/build.js",
    "test": "node .tooling/scripts/test.js --env=jsdom",
    "storybook": "start-storybook -c ./.tooling/storybook -s ./build/_dev -p 6006",
    "build-storybook": "build-storybook"
  }
