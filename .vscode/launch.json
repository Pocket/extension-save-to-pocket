{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "debug build",
      "env": {
        "PROJECT": "save-to-pocket"
      },
      "program": "${workspaceFolder}/node_modules/pocket-build-tools/scripts/build.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug CI build",
      "env": {
        "PROJECT": "save-to-pocket",
        "CI": "true",
        "API_KEY": "testApiKey"
      },
      "program": "${workspaceFolder}/node_modules/pocket-build-tools/scripts/build.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug publish",
      "env": {
        "PROJECT": "save-to-pocket",
        "CI": "true",
        "API_KEY": "${env:POCKET_API_KEY}",
        "POCKET_DROP_BOX_TOKEN": "${env:POCKET_DROP_BOX_TOKEN}"
      },
      "program": "${workspaceFolder}/node_modules/pocket-build-tools/scripts/publish.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "debug pr publish",
      "env": {
        "PROJECT": "save-to-pocket",
        "CI": "true",
        "API_KEY": "${env:POCKET_API_KEY}",
        "POCKET_DROP_BOX_TOKEN": "${env:POCKET_DROP_BOX_TOKEN}"
      },
      "program": "${workspaceFolder}/node_modules/pocket-build-tools/scripts/publish.js",
      "args": [
        "-pr"
      ]
    }
  ]
}